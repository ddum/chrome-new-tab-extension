{
  "name": "chrome-new-tab-extension",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint",
    "format": "eslint --fix",
    "test:watch": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "preversion": "run-p lint type-check",
    "version": "node scripts/sync-version.js && git add -A public/manifest.json",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major"
  },
  "dependencies": {
    "floating-vue": "^5.2.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.19"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@pinia/testing": "^0.1.3",
    "@tsconfig/node24": "^24.0.1",
    "@types/node": "^24.3.0",
    "@types/wicg-file-system-access": "^2023.10.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.34.0",
    "jsdom": "^24.0.0",
    "npm-run-all2": "^6.1.2",
    "typescript": "^5.9.2",
    "vite": "^5.1.4",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^1.3.1",
    "vue-tsc": "^3.0.6"
  }
}
